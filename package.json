  "lint-staged": {
    "linters": {
      "src/**/*.ts": [
        "npm run lint:ts",
        "git add"
      ],
      "src/**/*.less": [
        "npm run lint:style",
        "git add"
      ]
    },
    "ignore": [
      "src/assets/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  }
